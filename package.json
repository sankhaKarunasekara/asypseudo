{
  "name": "asypseudo",
  "displayName": "ASYPSEUDO",
  "description": "ASYCUDA PSEUDOCODE",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "asycuda-pseudocode",
        "aliases": [
          "ASYCUDA PSEUDOCODE",
          "asycuda-pseudocode"
        ],
        "extensions": [
          ".asypseudo"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "asycuda-pseudocode",
        "scopeName": "source.asypseudo",
        "path": "./syntaxes/asycuda-pseudocode.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "Your Theme Name",
        "uiTheme": "vs-dark",
        "path": "./themes/your-theme-color-theme.json",
        "scope": "variable.system.asypseudo",
        "settings": {
          "foreground": "#FF0000",
          "fontStyle": "bold"
        }
      },
      {
        "label": "Your Theme Name",
        "uiTheme": "vs-dark",
        "path": "./themes/your-theme-color-theme.json",
        "scope": "variable.user.asypseudo",
        "settings": {
          "foreground": "#0000FF",
          "fontStyle": "italic"
        }
      }
    ]
  },
  "dependencies": {
    "asypseudo": "file:"
  }
}